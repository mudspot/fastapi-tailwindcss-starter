{# templates/layout/base.html #}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FastApi TailwindCss Starter Kit</title>
    <link rel="icon" type="image/x-icon" href="{{url_for('assets', path='/css/favicon.ico')}}">
    <link rel="stylesheet" href="{{url_for('assets', path='/css/app.min.css')}}">
    <script defer type="application/javascript" src="{{url_for('assets', path='/js/alpine.min.js')}}"></script>
    <script type="application/javascript" src="{{url_for('assets', path='/js/htmx.min.js')}}"></script>
</head>

<body class="bg-gray-50 dark:bg-gray-900">
    {% block layout %}
    {% endblock layout %}
    <script type="application/javascript" src="{{url_for('assets', path='/js/flowbite.min.js')}}"></script>
</body>

</html>